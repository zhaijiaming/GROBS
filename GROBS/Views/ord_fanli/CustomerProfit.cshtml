            getFanliXiaoFeiWithQuery();
        });

        /*
            返利消费点击导出事件
        */

        $("#btn_export_flxf").on("click", function () {
            var _khid = $("#khid").val();
            var _req_ddid = "";
            var _req_bianhao = $("#req_bianhao").val().trim();
            console.log(_req_bianhao);
            $("#tbody_flxf tr").each(function () {
                console.log($(this).find(".bianhao_flxf").html());
