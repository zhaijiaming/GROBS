            }

            var str = "";

            //数据千分制
            function toThousands(s, n) {
                /*
                 * 参数说明：
                 * s：要格式化的数字
                 * n：保留几位小数
                 * */
                n = n > 0 && n <= 20 ? n : 2;
                s = parseFloat((s + "").replace(/[^\d\.-]/g, "")).toFixed(n) + "";
                var l = s.split(".")[0].split("").reverse(),
