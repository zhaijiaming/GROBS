            返利充值点击查询事件
        */
        $("#btn_req_flcz").on("click", function () {
            getFanliChongZhiWithQuery();
        });

        /*
           返利充值点击导出事件
       */
        $("#btn_export_flcz").on("click", function () {


            var _khid = $("#khid").val();
