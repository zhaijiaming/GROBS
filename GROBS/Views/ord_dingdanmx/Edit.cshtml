@using GROBS.App_Code
@using GROBS.Models
@model ord_dingdanmxViewModel
@{
    ViewBag.Title = "订单明细修改";
}
<script type="text/javascript">
    try { ace.settings.check('breadcrumbs', 'fixed') } catch (e) { }
    $(document).ready(function () {
        //submit
        $("#save_top,#save_bottom").on("click", function () {
            var CGSL = isPositiveInt($("#CGSL").val());
            if (CGSL != true) {
                alert("您输入的采购数量有错，请重新输入");
            } else {
                $("#form").submit();
            }
        });
        /*
            判断采购类型:判断是选择套包还是普通商品
            0：无
            1：普通
            2：套包
            3：促销
        */
        var cglx = $("#cglx").val();
        switch (cglx) {
            case "1":
                /*
                    选择商品
                */
                $("#containerSpxx").draggable({ handle: '.tuodong' }, { containment: [-1000, 50, 1300, 600] });
                //click
                $("#btn_select").on("click", function () {
                    $("#containerSpxx").show();
                    getSpxx();   //获取商品数据
                });
                //弹框关闭
                $("#container_close").click(function () {
                    $("#containerSpxx").hide();
                });
                //商品查询
                $("#spxx_search").on("click", function () {
                    $("#table_data tbody").html("");
                    getSpxx();
                });
                //双击取值
                $("#table_data tbody").on("dblclick", "tr", function () {
                    $("#spid").val($(this).find(".checkbox").val());
                    $("#SPBM").val($(this).find(".Daima").html());
                    $("#spmc").val($(this).find(".Mingcheng").html());
                    $("#Guige").val($(this).find(".Guige").html());
                    $("#XSBJ").val($(this).find(".XSBJ").html());
                    $("#XSDJ").val($(this).find(".XSBJ").html());
                    $("#HSL").val($(this).find(".Huansuanlv").html());
                    $("#HSBM").val($(this).find(".HSBM").html());
                    $("#JBDW").val($(this).find(".Danwei").html());
                    $("#XSDW").val($(this).find(".BaozhuangDW").html());
                    $("#CGSL").val("");
                    $("#Jine").val("");
                    $("#Zhekou").val("");
                    $("#zhekoulv_percent").val("");
                    $("#Zhekoulv").val("");
                    $("#CGSL").focus();
                    $("#containerSpxx").hide();
                });
                break;
            case "2":
                /*
                    选择套包
                */
                //修改样式
                $("#container_tb").draggable({ handle: '.tuodong' }, { containment: [-1000, 50, 1300, 600] });
                $(".label_spbm").html("套包代码");
                $(".label_spmc").html("套包");
                $("#display_Guige").hide();
                $("#display_HSL").hide();
                $("#display_Col1").hide();
                $("#display_JBDW").hide();
                //弹框显示
                $("#btn_select").on("click", function () {
                    $("#container_tb").show();
                    getTaobao();   //获取套包信息
                });
                //查看套包详情...
                $("#table_data_tb").on("click", "tr", function (e) {
                    if (e.target.tagName == "BUTTON") {
                        var tbid = $(this).find(".tb_checkbox").val();
                        //console.log(tbid);
                        getTaoBaoData(tbid);
                        $("#container_tbsp").show();
                    }
                });
                //弹框关闭
                $("#container_close_tb").click(function () {
                    $("#container_tb").hide();
                    $("#container_tbsp").hide();
                });
                //商品查询
                $("#btn_search_tb").on("click", function () {
                    $("#table_data_tb tbody").html("");
                    getTaobao();   //获取套包信息
                    $("#container_tbsp").hide();
                });
                //双击取值
                $("#table_data_tb tbody").on("dblclick", "tr", function () {
                    $("#spid").val($(this).find(".tb_checkbox").val());
                    $("#SPBM").val($(this).find(".tb_Daima").html());
                    $("#spmc").val($(this).find(".tb_Mingcheng").html());
                    //$("#Guige").val($(this).find(".Guige").html());
                    $("#XSBJ").val($(this).find(".tb_JiaXS").html());
                    $("#XSDJ").val($(this).find(".tb_JiaXS").html());
                    //$("#HSL").val($(this).find(".Huansuanlv").html());
                    //$("#HSBM").val($(this).find(".HSBM").html());
                    //$("#JBDW").val($(this).find(".Danwei").html());
                    $("#XSDW").val($(this).find(".tb_XSDW").html());
                    $("#CGSL").val("");
                    $("#Jine").val("");
                    $("#Zhekou").val("");
                    $("#zhekoulv_percent").val("");
                    $("#Zhekoulv").val("");
                    $("#CGSL").focus();
                    $("#container_tb").hide();
                    $("#container_tbsp").hide();
                });
                break;
            default:
                break;

        }
        //正则判断采购数量
        $("#CGSL").blur(function () {
            var CGSL = isPositiveInt($("#CGSL").val());
            if (CGSL != true) {
                alert("您输入的采购数量有错，请重新输入");
            }
        });
        //金额计算
        $("#CGSL,#XSBJ,#Jine").blur(function () {
            if ($("#CGSL").val() != "" && $("#XSBJ").val() != "" && $("#CGSL").val() != null && $("#XSBJ").val() != null) {
                var Jine = $("#CGSL").val() * $("#XSBJ").val();
                $("#Jine").val(toDecimal(Jine));
            } else {
                $("#Jine").val("0");
            }
        });
        ////给百分比折扣率赋值
        //if ($("#Jine").val() != "" && $("#Jine").val() != null && $("#Jine").val() != "0") {
        //    var zhekou = $("#Zhekou").val();
        //    var Jine = $("#Jine").val();
        //    var rel = 1 - zhekou / Jine;

        //    var rel_persent = rel * 100;
        //    $("#zhekoulv_percent").val(toDecimal(rel_persent));
        //}
        //折扣率计算
        $("#Zhekou").blur(function () {
            //if ($("#Jine").val() == "" || $("#Jine").val() == null || $("#Jine").val() == "0") {
            //    alert("金额不能为空或0");
            //    $("#CGSL").focus();
            //} else if ($("#Zhekou").val() != "" && $("#Zhekou").val() != null) {
            //    var zhekou = $("#Zhekou").val();
            //    var Jine = $("#Jine").val();
            //    var rel = 1 - zhekou / Jine;

            //    $("#Zhekoulv").val(toDecimal4(rel));

            //    var rel_persent = rel * 100;
            //    $("#zhekoulv_percent").val(toDecimal(rel_persent));
            //    cal_xiaoShouDanJia();//销售单价计算
            //}
        });
        //销售单价计算
        $("#zhekoulv_percent,#XSBJ").blur(function () {
            //cal_xiaoShouDanJia();
        });
        $("#XSDJ").blur(function () {
            if ($("#XSDJ").val() == "")
            {
                $("#XSDJ").val($("#Danjia").val());
            }

            if ($("#XSDJ").val() != $("#Danjia").val()) {
                var Jine = $("#CGSL").val() * $("#XSDJ").val();
                var zk = $("#CGSL").val() * ($("#XSBJ").val() - $("#XSDJ").val());
                $("#Jine").val(toDecimal(Jine).toFixed(2));
                $("#Zhekou").val(toDecimal(zk).toFixed(2));
                var zklv = ($("#XSDJ").val() / $("#XSBJ").val() * 100).toFixed(4);
                $("#zhekoulv_percent").val(zklv);
            }
            else
            {
                $("#Jine").val(@ViewData.Model.Jine);
                $("#Zhekou").val(@ViewData.Model.Zhekou);
                $("#zhekoulv_percent").val(@(ViewData.Model.Zhekoulv*100));
            }

        });
        //批复数量
        $("#pfsl").blur(function () {
            //cal_xiaoShouDanJia();
            if ($("#pfsl").val() == "")
            { $("#pfsl").val($("#CGSL").val()); }
        });
        //单击tr打钩
        $("#table_data,#table_data_tb").on("click", "tr", function (e) {
            if (e.target.tagName == "TD" || e.target.tagName == "SPAN") {
                if ($(this).find("input[type=checkbox]").is(':checked')) {
                    $(this).find(":checkbox").prop("checked", false);
                } else {
                    $("input[type=checkbox]").prop("checked", false);
                    $(this).find(":checkbox").prop("checked", true);
                }
            }
        });
    });
    //正则->正整数
    function isPositiveInt(num) {
        var pre_num = num;
        var g = /^[1-9]*[1-9][0-9]*$/;
        return (g.test(pre_num));
    }
    //销售单价计算
    function cal_xiaoShouDanJia() {
        if ($("#XSBJ").val() != "" && $("#XSBJ").val() != null && $("#XSBJ").val() != "0" && $("#Zhekoulv").val() != "" && $("#Zhekoulv").val() != null && $("#Zhekoulv").val() != "0") {
            var xsbj = $("#XSBJ").val();
            var zhekoulv = $("#Zhekoulv").val();
            var rel = xsbj * zhekoulv;
            $("#XSDJ").val(toDecimal(rel));
        }
    }
    //取小数点后两位
    function toDecimal(x) {
        var f = parseFloat(x);
        if (isNaN(f)) {
            return;
        }
        f = Math.round(x * 100) / 100;
        return f;
    }
    //取小数点后4位
    function toDecimal4(x) {
        var f = parseFloat(x);
        if (isNaN(f)) {
            return;
        }
        f = Math.round(x * 10000) / 10000;
        return f;
    }
    //获取套包
    function getTaobao() {
        var filltable = "";
        $.ajax({
            async: true,
            beforeSend: function () {
                ShowWait();
            },
            complete: function () {
                HiddenWait();
            },
            type: "POST",
            url: "/base_taobao/getTaobaoWithQuery",
            data: {
                req_tbdm: $("#req_tbdm").val(),
                req_tbmc: $("#req_tbmc").val(),
                req_gys: $("#req_gys").val(),
                cpxid: $("#cpx").val()
            },
            success: function (data) {
                var filltable = "";
                if (data != -1) {
                    for (var i in data) {
                        filltable = filltable + '<tr><td><label><input type="checkbox" class ="tb_checkbox mark_id" value="';
                        filltable = filltable + data[i].ID;
                        filltable = filltable + '"></label></td><td><span class="tb_Daima">';
                        filltable = filltable + data[i].Daima;
                        filltable = filltable + '</span></td><td><span class="tb_Mingcheng">';
                        filltable = filltable + data[i].Mingcheng;
                        filltable = filltable + '</span></td><td><span class="tb_Miaoshu">';
                        filltable = filltable + data[i].Miaoshu;
                        filltable = filltable + '</span></td><td><span class="tb_JiaXS">';
                        filltable = filltable + data[i].JiaXS;
                        filltable = filltable + '</span></td><td><span class="tb_JiaCG">';
                        filltable = filltable + data[i].JiaCG;
                        filltable = filltable + '</span></td><td><span class="tb_XSDW">';
                        filltable = filltable + data[i].XSDW;
                        filltable = filltable + '</span></td><td><span><button id="btn_detail_taobaosp" class="btn btn-link" style="padding-left:0px!important;">查看详情...</button></span></td></tr>';
                    }
                }
                else {
                    alert("获取信息出错，请联系管理员！");
                }
                $("#tbody_tb").html(filltable);
                markSelect($("#tbody_tb tr"));//记录选过的信息
            }
        });
    }
    //套包商品明细
    function getTaoBaoData(tbid) {
        var filltable = "";
        var tbId = tbid;
        $.post(
            "/base_taobaosp/getdetail",
                {
                    tbId: tbId
                },
               function (data) {
                   if (data.length > 0) {
                       for (var i in data) {
                           filltable = filltable + '<tr><td style="display:none;"><label><input type="checkbox" value="';
                           filltable = filltable + data[i].ID;
                           filltable = filltable + '"></label></td>';
                           filltable = filltable + '<td><span class="daima_tbsp">';
                           filltable = filltable + data[i].Daima;
                           filltable = filltable + '</span></td>';
                           filltable = filltable + '<td><span class="mingcheng_tbsp">';
                           filltable = filltable + data[i].Mingcheng;
                           filltable = filltable + '</span></td><td>';
                           filltable = filltable + data[i].Guige;
                           filltable = filltable + '</td><td>';
                           filltable = filltable + data[i].Danwei;
                           filltable = filltable + '</td><td><span class="tbsp_jiaxs">';
                           filltable = filltable + data[i].JiaXS;
                           filltable = filltable + '</span></td><td><span class="tbsp_jiacg">';
                           filltable = filltable + data[i].JiaCG;
                           filltable = filltable + '</span></td><td><span class="tbsp_shuliang">';
                           filltable = filltable + data[i].Shuliang;
                           filltable = filltable + '</span></td></tr>';
                       }
                       $("#tbody_tbsp").html(filltable);
                   } else {
                       $("#tbody_tbsp").empty();
                   }
               }
            );
    }
    //获取商品信息
    function getSpxx() {
        var filltable = "";
        $.ajax({
            async: true,
            beforeSend: function () {
                ShowWait();
            },
            complete: function () {
                HiddenWait();
            },
            type: "POST",
            url: "/base_shangpinxx/getSpxxWithQuery",
            data: {
                req_spbm: $("#req_spbm").val(),
                req_spmc: $("#req_spmc").val(),
                req_guige: $("#req_guige").val(),
                cpxid: $("#cpx").val()
            },
            success: function (data) {
                var filltable = "";
                if (data != -1) {
                    for (var i in data) {
                        filltable = filltable + '<tr><td><label><input type="checkbox" class ="checkbox mark_id" value="';
                        filltable = filltable + data[i].ID;
                        filltable = filltable + '"></label></td><td><span class="Daima">';
                        filltable = filltable + data[i].Daima;
                        filltable = filltable + '</span></td><td><span class="Mingcheng">';
                        filltable = filltable + data[i].Mingcheng;
                        filltable = filltable + '</span></td><td><span class="Guige">';
                        filltable = filltable + data[i].Guige;
                        filltable = filltable + '</span></td><td><span class="XSBJ">';
                        filltable = filltable + data[i].JiaXS;
                        filltable = filltable + '</span></td><td><span class="Danwei">';
                        filltable = filltable + data[i].Danwei;
                        filltable = filltable + '</span></td><td><span class="BaozhuangDW">';
                        filltable = filltable + data[i].BaozhuangDW;
                        filltable = filltable + '</span></td><td><span class="Huansuanlv">';
                        filltable = filltable + data[i].Huansuanlv;
                        filltable = filltable + '</span></td><td><span class="HSBM">';
                        filltable = filltable + data[i].Col1;
                        filltable = filltable + '</span></td></tr>';
                    }
                }
                else {
                    alert("获取商品信息出错，请联系管理员！");
                }
                $("#tbody_sp").html(filltable);
                markSelect($("#tbody_sp tr"));//记录选过的信息
            }
        });
    }
    //记录选过的信息
    function markSelect(selector) {
        var selector = selector;
        selector.each(function () {
            if ($("#spid").val() == $(this).find(".mark_id").val()) {
                $(this).find("input[type=checkbox]").prop("checked", true);
            }
        });
    }
</script>
<div class="breadcrumbs" id="breadcrumbs">
    <ul class="breadcrumb">
        <li>
            <i class="icon-home home-icon"></i>
            <a href="/Home">首页</a>
        </li>
        <li>
            <a href="/ord_dingdan/Edit/@ViewData.Model.DDID">订单明细</a>
        </li>
        <li class="active"> 编辑</li>
    </ul>
</div>

<div class="page-content">
    <div class="page-header">
        <button type="button" class="btn btn-app btn-xs btn-primary radius-4" onclick="javascript: history.back(-1); "><i class="icon-reply icon-only"></i><b>返回</b></button>
        <button type="button" id="save_top" class="btn btn-app btn-xs btn-primary radius-4"><i class="icon-save"></i><b>更新</b></button>
    </div>
    <div class="wrapper" style="background: url('/Imgs/bg-cloud.png') repeat;">
        <div class="container">
            <div class="row">
                <div class="col-sm-8 col-sm-offset-2">
                    <div class="widget-box radius-4">
                        <div class="widget-header">
                            <h4><b>订单明细</b></h4>
                        </div>
                        <div class="widget-body">
                            <div class="widget-main no-padding">
                                @using (Html.BeginForm("Update", "ord_dingdanmx", FormMethod.Post, new { @role = "form", @class = "form-horizontal", @id = "form" }))
                                {
                                    @Html.AntiForgeryToken()
                                    <input type="hidden" name="id" value="@ViewData.Model.ID" />
                                    <input type="hidden" name="ddid" id="ddid" value="@ViewData.Model.DDID" />
                                    <input type="hidden" name="spid" id="spid" value="@ViewData.Model.SPID"/>
                                    <input type="hidden" id="MakeMan" name="MakeMan" value=@Session["user_id"] />
                                    <input type="hidden" name="MakeDate" value=@string.Format("{0:yyyy-MM-dd}",DateTime.Now) />
                                    <input type="hidden" name="Zhekoulv" id="Zhekoulv" value="@ViewData.Model.Zhekoulv"/>
                                    <input type="hidden" id="cpx" value="@ViewBag.cpx" />
                                    <input type="hidden" id="cglx" value="@ViewBag.cglx" />
                                    <fieldset>
                                        @*商品名称*@
                                        <div class="form-group">
                                            <label class="col-sm-3 control-label"><i class="red glyphicon glyphicon-asterisk"></i> <b class="label_spmc">商品</b></label>
                                            <div class="col-sm-8">
                                                <div class="input-group">
                                                    <input id="spmc" name="spmc" placeholder="请选择商品" class="form-control" type="text" value="@ViewData.Model.SPMC">
                                                    <span class="input-group-btn">
                                                        <button type="button" id="btn_select" class="btn btn-primary btn-sm">
                                                            选择
                                                            <i class="icon-hand-up icon-on-right bigger-110"></i>
                                                        </button>
                                                    </span>
                                                </div>
                                            </div>
                                        </div>
                                        @*商品代码*@
                                        <div class="form-group">
                                            <label for="SPBM" class="col-sm-3 control-label"><i class="red glyphicon glyphicon-asterisk"></i> <b class="label_spbm">商品代码</b></label>
                                            <div class="col-sm-8">
                                                <input type="text" id="SPBM" name="SPBM" class="form-control" placeholder="请选择商品" value="@ViewData.Model.SPBM"/>
                                            </div>
                                        </div>
                                        @*规格*@
                                        <div id="display_Guige" class="form-group">
                                            <label for="Guige" class="col-sm-3 control-label"><b>规格</b></label>
                                            <div class="col-sm-8">
                                                <input type="text" id="Guige" name="Guige" class="form-control" placeholder="请选择商品" value="@ViewData.Model.Guige"/>
                                            </div>
                                        </div>
                                        @*采购数量*@
                                        <div class="form-group">
                                            <label for="CGSL" class="col-sm-3 control-label"><b>采购数量</b></label>
                                            <div class="col-sm-8">
                                                <input type="text" id="CGSL" name="CGSL" class="form-control" placeholder="请输入采购数量" value="@ViewData.Model.CGSL" />
                                            </div>
                                        </div>
                                        @*可用量*@
                                        <div class="form-group">
                                            <label for="kysl" class="col-sm-3 control-label"><b>可用量</b></label>
                                            <div class="col-sm-8">
                                                <input type="text" id="kysl" name="kysl" class="form-control" readonly value="@ViewBag.kysl" />
                                            </div>
                                        </div>
                                        @*批复数量*@
                                        <div class="form-group">
                                            <label for="pfsl" class="col-sm-3 control-label"><b>批复数量</b></label>
                                            <div class="col-sm-8">
                                                <input type="text" id="pfsl" name="pfsl" class="form-control" placeholder="请输入批复数量" value="@ViewData.Model.PFSL" />
                                            </div>
                                        </div>
                                        @*销售报价*@
                                        <div class="form-group">
                                            <label for="XSBJ" class="col-sm-3 control-label"><b>销售报价</b></label>
                                            <div class="col-sm-8">
                                                <input type="text" id="XSBJ" readonly name="XSBJ" class="form-control" placeholder="请选择商品" value="@ViewData.Model.XSBJ" />
                                            </div>
                                        </div>
                                        @*销售单价*@
                                        <div class="form-group">
                                            <label for="Danjia" class="col-sm-3 control-label"><b>销售单价</b></label>
                                            <div class="col-sm-8">
                                                <input type="text" id="Danjia" name="Danjia" class="form-control" placeholder="请选择商品" value="@ViewData.Model.Danjia" />
                                            </div>
                                        </div>
                                        @*批复价格*@
                                        <div class="form-group">
                                            <label for="XSDJ" class="col-sm-3 control-label"><b>批复价格</b></label>
                                            <div class="col-sm-8">
                                                <input type="text" id="XSDJ" name="XSDJ" class="form-control" placeholder="请输入批复价格" value="@ViewData.Model.XSDJ" />
                                            </div>
                                        </div>
                                        @*换算率*@
                                        <div id="display_HSL" class="form-group">
                                            <label for="HSL" class="col-sm-3 control-label"><b>换算率</b></label>
                                            <div class="col-sm-8">
                                                <input type="text" id="HSL" name="HSL" class="form-control" placeholder="请选择商品" value="@ViewData.Model.HSL" />
                                            </div>
                                        </div>
                                        @*换算编码*@
                                        <div id="display_Col1" class="form-group">
                                            <label for="HSBM" class="col-sm-3 control-label"><b>换算编码</b></label>
                                            <div class="col-sm-8">
                                                <input type="text" id="HSBM" name="HSBM" class="form-control" placeholder="请选择商品" value="@ViewData.Model.HSBM" />
                                            </div>
                                        </div>
                                        @*基本单位*@
                                        <div id="display_JBDW" class="form-group">
                                            <label for="JBDW" class="col-sm-3 control-label"><b>基本单位</b></label>
                                            <div class="col-sm-8">
                                                <input type="text" id="JBDW" name="JBDW" class="form-control" placeholder="请选择商品" value="@ViewData.Model.JBDW" />
                                            </div>
                                        </div>
                                        @*销售单位*@
                                        <div class="form-group">
                                            <label for="XSDW" class="col-sm-3 control-label"><b>销售单位</b></label>
                                            <div class="col-sm-8">
                                                <input type="text" id="XSDW" name="XSDW" class="form-control" placeholder="请选择商品" value="@ViewData.Model.XSDW" />
                                            </div>
                                        </div>
                                        <hr />                                 
                                        @*发货数量*@
                                        <div class="form-group">
                                            <label for="FHSL" class="col-sm-3 control-label"><b>发货数量</b></label>
                                            <div class="col-sm-8">
                                                <input type="text" id="FHSL" readonly name="FHSL" class="form-control" placeholder="请输入发货数量" value="@ViewData.Model.FHSL" />
                                            </div>
                                        </div>
                                        @*金额*@
                                        <div class="form-group">
                                            <label for="Jine" class="col-sm-3 control-label"><b>金额</b></label>
                                            <div class="col-sm-8">
                                                <input type="text" id="Jine" name="Jine" class="form-control" placeholder="请输入金额" value="@ViewData.Model.Jine" />
                                            </div>
                                        </div>
                                        @*折扣额*@
                                        <div class="form-group">
                                            <label for="Zhekou" class="col-sm-3 control-label"><b>折扣额</b></label>
                                            <div class="col-sm-8">
                                                <input type="text" id="Zhekou" name="Zhekou" class="form-control" placeholder="请输入折扣额" value="@ViewData.Model.Zhekou" />
                                            </div>
                                        </div>
                                        @*折扣率*@
                                        <div class="form-group">
                                            <label for="Zhekoulv" class="col-sm-3 control-label"><b>折扣率</b></label>
                                            <div class="col-sm-8">
                                                <div class="input-group">
                                                    <input type="text" id="zhekoulv_percent" name="zhekoulv_percent" class="form-control" placeholder="请输入折扣率" value="@(ViewData.Model.Zhekoulv*100)"/>
                                                    <span class="input-group-addon input-group-sm"><b>%</b></span>
                                                </div>
                                            </div>
                                        </div>
                                        
                                        <hr />
                                        @*录入日期*@
                                        <div class="form-group">
                                            <label class="col-sm-3 control-label" for="MakeDate"><b>录入日期</b></label>
                                            <div class="col-sm-8">
                                                <div class="input-group">
                                                    <input id="MakeDate" disabled="disabled" class="form-control" value="@string.Format("{0:yyyy-MM-dd}",ViewData.Model.MakeDate)" type="text">
                                                    <span class="input-group-addon">
                                                        <i class="icon-calendar bigger-110"></i>
                                                    </span>
                                                </div>
                                            </div>
                                        </div>
                                        @*录入人*@
                                        <div class="form-group">
                                            <label class="col-sm-3 control-label" for="MakeMan"><b>录入人</b></label>
                                            <div class="col-sm-8">
                                                <div class="input-group">
                                                    <input placeholder="录入人" disabled="disabled" class="form-control" type="text" value="@Html.GetDataValue_ID("userinfo", "全名", (int)ViewData.Model.MakeMan)">
                                                    <span class="input-group-addon">
                                                        <i class="icon-user bigger-110"></i>
                                                    </span>
                                                </div>
                                            </div>
                                        </div>
                                    </fieldset>
                                    <div class="form-actions center">
                                        <button id="save_bottom" class="btn btn-primary" type="button">
                                            <b>更新</b>
                                            <i class="icon-save bigger-110"></i>
                                        </button>
                                    </div>
                                }
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
